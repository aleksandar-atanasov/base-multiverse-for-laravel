{
    "name": "aleksandaratanasov/base-multiverse-for-laravel",
    "description": "Versatile base converter for Laravel.",
    "keywords": [
        "php-base-multiverse",
        "php"
    ],
    "homepage": "https://github.com/aleksandar-atanasov/base-multiverse-for-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandar Atanasov",
            "email": "aleksandar.atanasov91@yahoo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "dev-develop",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Aleksandar\\Multiverse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aleksandar\\Multiverse\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aleksandar\\Multiverse\\BaseMultiverseServiceProvider"
            ],
            "aliases": {
                "Aleksandar\\Multiverse\\Facades": "Aleksandar\\Multiverse\\Facades\\BaseConverter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
